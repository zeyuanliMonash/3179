        
{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/zeyuanliMonash/3179/main/earthquake_data.csv"},
    "vconcat": [{
      "width": 680,
      "height":200,
      "mark": "area",
      "encoding": {
        "x": {
          "field": "data_time",
          "type": "temporal",
          "timeUnit":"yearquarter",
          "scale": {"domain": {"param": "brush"}},
          "axis": {"title": "sdaasdas"}
        },
        "y": {
          "aggregate": "count",
         
          "title": "The count of earthquake"
          
        },
         "color": {
        "field": "magnitude",
        "type": "quantitative",
        "scale": {
         
          "scheme": "reds"
        },
        "title": "magnitude"
      }
       
      }
    }, {
      "width": 480,
      "height": 60,
      "mark": "area",
      "params": [{
        "name": "brush",
        "select": {"type": "interval", "encodings": ["x"]}
      }],
      "encoding": {
        "x": {
          "field": "data_time",
          "timeUnit":"yearquarter",
          "type": "temporal"
        },
        "y": {
          "aggregate": "count"
          
         
        
        },
         "color": {
        "field": "magnitude",
        "type": "quantitative",
        "scale": {
         
          "scheme": "reds"
        },
        "title": "Year"
      }
      }
    }]
  }
